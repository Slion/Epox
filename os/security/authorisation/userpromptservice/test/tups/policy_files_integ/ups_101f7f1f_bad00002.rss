			options = KAlways;
			flags = 2;
			},
		POLICY
			{
			// Silently denial  
			sid_list = {0x1234abcd,0x80002222,0x80001234};
			destination = "paris";
			options = KNever;
			policyevaluator = 0xE1111E10;
			dialogcreator = 0xE1111D20;
			flags = 3;
			},				
		POLICY
			{
			// yes or no
			sid_classes = KUnprotectedSids;
			destination = "*";
			options = KYes|KNo;
			policyevaluator = 0xE1111E10;
			dialogcreator = 0xE1111D20;
			flags = 4;
			}
		};
	}

