 #include <platform_paths.hrh>
TARGET          dbus_glib_test_service.exe
TARGETTYPE      exe
CAPABILITY      NetworkServices
UID             0x100039CE 0x0126209C

EPOCHEAPSIZE 4 67108864   // min. 4B  max 64MB

// Macros
MACRO 			SYMBIAN
MACRO 			HAVE_CONFIG_H
//MACRO			DBUS_BUILD_TESTS //To enable tests uncomment this line
MACRO			_DEBUG			//To enable debug and verbose mode

//START RESOURCE  dbus-daemon_reg.rss
//#ifdef WINSCW
//TARGETPATH 	    /private/10003a3f/apps
//#else
//TARGETPATH 	    /private/10003a3f/import/apps
//#endif
//END //RESOURCE

SOURCEPATH 		../test


// Source files
SOURCE test-service.c

// User include paths
USERINCLUDE     ../dbus ..

// System include paths




  

OS_LAYER_SYSTEMINCLUDE
OS_LAYER_LIBC_SYSTEMINCLUDE      
OS_LAYER_GLIB_SYSTEMINCLUDE      
OS_LAYER_DBUS_SYSTEMINCLUDE      

// Library dependencies
#ifdef GCCE
STATICLIBRARY  libcrt0_gcce.lib
#else
STATICLIBRARY  libcrt0.lib
#endif
LIBRARY        libc.lib 
LIBRARY        euser.lib  // Needed in order to use Symbian services

LIBRARY		libm.lib
LIBRARY		libpthread.lib
LIBRARY         libdbus.lib
LIBRARY			libglib.lib
LIBRARY			libgmodule.lib

LIBRARY			libgobject.lib
LIBRARY			libgthread.lib
LIBRARY			libdbus-glib.lib

SMPSAFE
