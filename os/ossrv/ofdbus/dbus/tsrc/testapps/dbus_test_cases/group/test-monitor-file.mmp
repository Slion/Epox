 #include <platform_paths.hrh>
TARGET          test_monitor_file.exe
TARGETTYPE      exe
CAPABILITY      NetworkServices
UID             0x100039CE 0x05B9458F

EPOCHEAPSIZE 4 67108864   // min. 4B  max 64MB


MACRO 			SYMBIAN
MACRO 			HAVE_CONFIG_H


SOURCEPATH 		..


// Source files
SOURCE test-monitor-file.c


// User include paths
USERINCLUDE     ../../../../dbus  ../../../..

// System include paths


OS_LAYER_SYSTEMINCLUDE 
OS_LAYER_LIBC_SYSTEMINCLUDE       
OS_LAYER_DBUS_SYSTEMINCLUDE

// Library dependencies
#ifdef GCCE
STATICLIBRARY  libcrt0_gcce.lib
#else
STATICLIBRARY  libcrt0.lib
#endif
LIBRARY        libc.lib 
LIBRARY        euser.lib  // Needed in order to use Symbian services

LIBRARY		   libpthread.lib
LIBRARY      libdbus.lib
LIBRARY      libdbus-utils.lib



SMPSAFE
