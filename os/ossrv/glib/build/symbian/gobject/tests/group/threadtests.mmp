#include <platform_paths.hrh>
target			threadtests.exe
targettype			exe

UID               0x1000008d  0xe000001a

#ifdef EKA2
CAPABILITY ALL -TCB
//VENDORID        VID_DEFAULT
#endif

//The src directory is added to the (user)include path by default
userinclude			../../../../../gobject/tests
systeminclude		../../../../../gobject/tests
userinclude			../../../tests/inc
//The top level directory is added to the include path by default
//systeminclude			.. ../glib 

//Adding the Symbian and PIPS include path
systeminclude			/epoc32/include
systeminclude			/epoc32/include/stdapis
systeminclude			/epoc32/include/stdapis/glib-2.0

sourcepath			../../../../../gobject/tests
source			 threadtests.c

//Adding some default libraries
library			euser.lib libc.lib libglib.lib libgobject.lib libgthread.lib
staticlibrary 	libcrt0.lib

SMPSAFE
