//!                CMMFDevVideoRecord::NewL(MMMFDevVideoRecordObserver{ref});
//!                CMMFDevVideoRecord::SelectEncoderL(TUid);
//!                CMMFDevVideoRecord::SetInputFormatL(THwDeviceId, const TUncompressedVideoFormat {ref}, const TSize{ref});
