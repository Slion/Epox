//!                CMMFDevSound::InitializeL(MDevSoundObserver {ref}, TFourCC, TMMFState)
//!                CMMFDevSound::SetPrioritySettings(TMMFPrioritySettings {ref})
//!                CMMFDevSound::PlayToneSequenceL(TDesC8 {ref})
