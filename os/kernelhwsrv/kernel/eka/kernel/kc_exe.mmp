#include		<variant.mmh>
#include		"kern_int.mmh"

target			VariantTarget(kc_exe,lib)
targettype		klib

#if defined(MARM)
sourcepath		arm
source			k_entry.cia
#elif defined(X86)
sourcepath		x86
source			k_entry.cpp
source			k_entry.cia
#elif defined(WINS)
sourcepath		win32
source			k_entry.cpp
#ifdef CW32
start wins
win32_headers
end
#endif
#endif

capability		all

VENDORID 0x70000001

SMPSAFE
