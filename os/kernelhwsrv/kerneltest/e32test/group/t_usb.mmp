TARGET        t_usb.exe
TARGETTYPE    EXE

SOURCEPATH	  ../device
SOURCE        t_usb.cpp
SOURCE        t_usbco2.cpp

OS_LAYER_SYSTEMINCLUDE_SYMBIAN
LIBRARY       euser.lib hal.lib
LIBRARY       efsrv.lib

CAPABILITY    ALL

VENDORID      0x70000001

// Larger user heap than default: maximum = 3MB
EPOCHEAPSIZE  0x1000 0x300000

// 0x101fe1db to pretend to be USB Manager (needed for the OTG drivers)
UID 0x0 0x101fe1db

SMPSAFE
