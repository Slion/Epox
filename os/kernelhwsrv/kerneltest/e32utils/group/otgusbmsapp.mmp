TARGET         		otgusbmsapp.exe
TARGETTYPE     		EXE

MACRO BUILD_OTG_USBMSAPP=1

SOURCEPATH		../usbmsapp
SOURCE 	       	usbmsapp.cpp
	
LIBRARY			efsrv.lib euser.lib hal.lib usbcsc_bil.lib
OS_LAYER_SYSTEMINCLUDE_SYMBIAN

capability	all

unpaged

// 0x101fe1db to pretend to be USB Manager (needed for the OTG drivers)
UID 0x0 0x101fe1db
